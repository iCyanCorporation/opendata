{
  "url": "https://connpass.com/api/v1/event/",
  "method": "GET",
  "sleep_time": 1.0,
  "extraction": {
    "headers": {
      "Accept": "application/json",
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/123.0.0.0 Safari/537.36",
      "Accept-Language": "en-US,en;q=0.9,ja;q=0.8",
      "Referer": "https://connpass.com/",
      "Origin": "https://connpass.com"
    },
    "params": {
      "count": 100,
      "order": 2,
      "keyword": "tech"
    },
    "data_path": "events",
    "columns": [
      "title",
      "started_at",
      "ended_at",
      "place",
      "address",
      "description",
      "event_url",
      "accepted",
      "limit",
      "catch",
      "hash_tag"
    ],
    "filters": [
      {
        "column": "started_at",
        "operator": ">",
        "value": "2023-01-01"
      }
    ]
  }
}
